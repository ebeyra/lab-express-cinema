<a href="/movies">Go back</a>
<br />
<img src={{this.image}} />
<h2>{{this.title}}</h2>
<h3>Director: {{this.director}}</h3>
<h4>Stars</h4>
{{#each this.stars}}
  <ul>
    <li>{{this}}</li>
  </ul>
{{/each}}
<br />
<p>{{this.description}}</p>
<p>
  {{#each this.showtimes}}
    {{this}}
    |
  {{/each}}
</p>